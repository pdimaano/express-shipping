{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "http://localhost:3000/shipments.order.schema.json",
    "title": "Shipping Order",
    "description": "Order for a shipments.",
    "type": "object",
    "properties": {
      "name": {"type": "string"},
      "productId": {
        "type": "integer",
        "minimum": 1000
      },
      "addr": {"type": "string"},
      "zip": {"type": "string"}
    },
    "additionalProperties": false,
    "required": [
      "name",
      "productId",
      "addr",
      "zip"
    ]
  }